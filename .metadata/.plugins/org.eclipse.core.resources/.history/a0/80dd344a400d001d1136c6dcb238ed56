package jiyeon;

import java.util.HashMap;
import java.util.Map;


public class CompanyMap {

	//관리할 사원 목록
	Map< String, Employee > map = new HashMap<>();
	
	//사원 추가(Create)
	public void insert(Employee e) {
		map.put(e.getEn(),e);
	}
	
	//사원 조회(Read)
	//
	public Employee[] getAllEmployees() {
		map.values()
	}
	
}
